@model IEnumerable<RedSocial.Models.USUARIOS>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear", "Crear")
</p>
<table class="table">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.NombreUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApellidoUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TelefonoUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CorreoUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaNacimiento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Genero)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PassUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha_Registro)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rol_ID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UsuarioID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NombreUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ApellidoUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TelefonoUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CorreoUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaNacimiento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Genero)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PassUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_Registro)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rol_ID)
        </td>
        <td>
            @Html.ActionLink("Editar", "Editar", new { id=item.UsuarioID}) |
            @Html.ActionLink("Detalles", "Detalles", new { id = item.UsuarioID }) |
            @Html.ActionLink("Eliminar", "Eliminar", new { id = item.UsuarioID })
        </td>
    </tr>
}

</table>
